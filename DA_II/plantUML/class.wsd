  #diemCPA
  thongKe(): void
}
class HeDaoTao {
  +maHeDaoTao
  +tenHeDaoTao
}
class KhoaHoc {
  +maKhoaHoc
  +tenKhoaHoc
}
class NganhDaoTao {
  +maNganh
  +tenNganh
}
SinhVien -- ThongTinHocTap
SinhVien "n" -- "1" HeDaoTao
SinhVien "n" -- "1" KhoaHoc
SinhVien "n" -- "1" NganhDaoTao

class GiangVien {
  #maGiangVien
  #tenGiangVien
  #gioiTinh
  #chuyenNganh
}
