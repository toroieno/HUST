GiangVien "1" -- "n" LopHoc

class GiangVienBoMon {
  #maGiangVien
  #tenGiangVien
  #gioiTinh
  #chuyenNganh
}
class LopHoc {
  #maLop
  #tenLop
  #soLuong
  #maKhoaHoc
  #maNganh
  #maGiangVien
}

class TruongPhong {
  #maTruongPhong
  #tenTruongPhong
  #ngaySinh

}
class GiaoVu {
  #maGiaoVu
  #tenGiaoVu
  #ngaySinh

}
class Admin {

}
class FormDangKy {
  #maForm
  #maLoaiForm
  thongKe(): void
}
FormDangKy -- SinhVien
FormDangKy -- TruongPhong

GiaoVu -- SinhVien: manage
GiaoVu -- GiangVien
GiaoVu -- FormDangKy
SinhVien "n" -- "1" LopHoc

GiangVien <|-- GiangVienBoMon
GiangVien <|-- Giang
@enduml
